         <children>
            <Button mnemonicParsing="false" onAction="#back" text="Back" />
            <HBox prefHeight="42.0" prefWidth="600.0">
               <children>
                  <Label prefHeight="21.0" prefWidth="35.0" text="TOP">
                     <HBox.margin>
                        <Insets left="5.0" top="10.0" />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="customerNumberBox" prefHeight="31.0" prefWidth="70.0" promptText="(5, 10...)">
                     <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </HBox.margin>
                  </TextField>
                  <Label prefHeight="21.0" prefWidth="183.0" text="Khách hàng chi tiêu trong">
                     <HBox.margin>
                        <Insets bottom="5.0" top="10.0" />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="dayCustomerBox" prefHeight="31.0" prefWidth="74.0" promptText="(1, 2...)">
                     <HBox.margin>
                        <Insets left="5.0" top="5.0" />
                     </HBox.margin>
                  </TextField>
                  <Label alignment="CENTER" prefHeight="21.0" prefWidth="141.0" text="NGÀY gần nhất" textAlignment="CENTER">
