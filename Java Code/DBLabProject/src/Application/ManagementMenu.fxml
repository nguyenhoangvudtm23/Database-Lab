<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="anchorPane" style="-fx-background-color: #88BDBC;" stylesheets="@ManagementMenu.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Application.ManagementMenuController">
   <children>
      <VBox prefHeight="390.0" prefWidth="591.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <JFXButton id="button1" fx:id="productButton" mnemonicParsing="false" onAction="#switchProductScene" prefHeight="8000.0" prefWidth="8000.0" stylesheets="@ManagementMenu.css" text="Kho sản phẩm" VBox.vgrow="ALWAYS" />
            <JFXButton id="button2" fx:id="ingredientButton" mnemonicParsing="false" onAction="#switchIngredientScene" prefHeight="8000.0" prefWidth="8000.0" stylesheets="@ManagementMenu.css" text="Kho nguyên liệu" VBox.vgrow="ALWAYS" />
            <JFXButton id="button3" fx:id="customerButton" mnemonicParsing="false" onAction="#switchCustomerScene" prefHeight="8000.0" prefWidth="8000.0" stylesheets="@ManagementMenu.css" text="Khách hàng" VBox.vgrow="ALWAYS" />
            <JFXButton id="button4" fx:id="supplierButton" mnemonicParsing="false" onAction="#switchSupplierScene" prefHeight="8000.0" prefWidth="8000.0" stylesheets="@ManagementMenu.css" text="Đối tác" VBox.vgrow="ALWAYS" />
            <JFXButton id="button5" fx:id="orderButton" mnemonicParsing="false" onAction="#switchOrderScene" prefHeight="8000.0" prefWidth="8000.0" stylesheets="@ManagementMenu.css" text="Hoá đơn bán hàng" VBox.vgrow="ALWAYS" />
            <JFXButton id="button6" fx:id="buyOrderButton" mnemonicParsing="false" onAction="#switchBuyOrderScene" prefHeight="8000.0" prefWidth="8000.0" stylesheets="@ManagementMenu.css" text="Hoá đơn nhập hàng" VBox.vgrow="ALWAYS" />
         </children>
      </VBox>
   </children>
</AnchorPane>
